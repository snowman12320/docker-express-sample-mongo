<!DOCTYPE html>
<html>
  <head>
    <title id="page-title"></title>
  </head>
  <body>
    <h1 id="header"></h1>
    <p id="welcome"></p>
    <a id="link" href="#">gemini</a>

    <script>
      // 从 URL 参数获取 baseURL
      const baseURL = new URLSearchParams(window.location.search);
      const title = "首頁";

      // 设置标题
      document.getElementById("page-title").textContent = title;

      // 设置链接
      const link = document.getElementById("link");
      link.href = baseURL + "/gemini";
      link.textContent = "gemini";
    </script>
  </body>
</html>
